*******************************************************************************/

/****** Step 1 ********/
INSERT INTO ${TMP_BASE}_VALID_COMPANY_JOC_TMP
SELECT vcj.joc, vcj.ticker
FROM ${TABLE_VALID_COMPANY_JOC} vcj
JOIN ${TABLE_COMPANY_SUBSIDIARY_TICKER} c on c.ticker = vcj.ticker
