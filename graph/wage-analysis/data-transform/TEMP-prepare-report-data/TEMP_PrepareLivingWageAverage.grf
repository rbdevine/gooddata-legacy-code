<Node dbConnection="JDBC0" enabled="enabled" guiName="Populate Living Wage Average" guiX="15" guiY="105" id="POPULATE_LIVING_WAGE_AVERAGE" type="DB_EXECUTE">
<attr name="sqlQuery"><![CDATA[INSERT INTO ${LIVING_WAGE_AVERAGES_TABLENAME}
select state_county_fips, living_wage_hourly
from ${TABLE_FIPS_DATA}
group by state_county_fips, living_wage_hourly_by_state_county_fips;]]></attr>
