<attr name="sqlQuery"><![CDATA[/********************************************************************************
  ${LIVING_WAGE_AVERAGES_TABLENAME} 
  average cost os living index for state_county_fips.
  this is a transitional program to temporarily connect with old architecture for
  generating living wage reports
*********************************************************************************/
DROP TABLE IF EXISTS ${LIVING_WAGE_AVERAGES_TABLENAME};
CREATE TABLE ${LIVING_WAGE_AVERAGES_TABLENAME} (
  STATE_COUNTY_FIPS VARCHAR(5) NOT NULL,
  LIVING_WAGE_AVG FLOAT
);
]]></attr>
