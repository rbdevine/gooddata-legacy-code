<attr name="transform"><![CDATA[//#CTL2

// Transforms input record into output record.
function integer transform() {
	$out.0.COMPANYTICKER = $in.0.COMPANYTICKER;
	if ($in.0.LINKEDINCOMPANYNAME != null)
	{	
		$out.0.LINKEDINCOMPANYNAME = replace($in.0.LINKEDINCOMPANYNAME,"[|]","");
	}
	$out.0.LINKEDINCOMPANYID = $in.0.LINKEDINCOMPANYID;
	$out.0.JOBLOCATION = replace($in.0.JOBLOCATION,"[|]","");
	if ($in.0.JOBLOCATION != null)
	{
		string cityName = "";
		string state = "";
		string stateAbbrev = "";
		string stateName = "";
		string [] locationArray = null;
		if (indexOf($in.0.JOBLOCATION,",") > 0)
		{
			locationArray = split($in.0.JOBLOCATION,",");
			cityName = upperCase(trim(locationArray[0]));
			if (length(locationArray) > 1)
			{
				// location format [CITY], [STATE ABBEV] and [CITY], [STATE NAME]
				state = upperCase(trim(locationArray[1]));
				if (length(state) > 2)
				{
					//handles case where statename is actually US-STATE ABBREV
					if(indexOf(state,"-") > 0)
					{
						string[] stateArray = split(state,"-");
						if(trim(stateArray[0]) == "US")
						{
							stateAbbrev =  trim(stateArray[1]);
						}
						else
						{
							stateName = state;
						}	
					}
					else
					{
						stateName = state;
					}
				}
				else
				{
					stateAbbrev = state;
				}
			}
		}
		else if (indexOf($in.0.JOBLOCATION,"-") > 0)
		{
			locationArray = split($in.0.JOBLOCATION,"-");
			if (length(locationArray) == 2)
			{
				//[STATE-ABBREV]-[CITY]
				cityName = upperCase(trim(locationArray[1]));
				stateAbbrev = upperCase(trim(locationArray[0]));
			}
			else if (length(locationArray) == 3)
			{
				if (trim(locationArray[0]) == "US")
				{
					//US-[STATE-ABBREV]-[CITY]
					cityName = upperCase(trim(locationArray[2]));
					stateAbbrev = upperCase(trim(locationArray[1]));
				}
				else
				{
					//[CITY]-[STATE-ABBREV]
					cityName = upperCase(trim(locationArray[0]));
					stateAbbrev = upperCase(trim(locationArray[1]));	
				}
			}
			else if(length(locationArray) == 4)
			{
				//SF-Corporate-California-US
				cityName = upperCase(trim(locationArray[0]));
				if (cityName == "SF")
				{
					cityName = "SAN FRANCISCO";	
				}
				stateName = upperCase(trim(locationArray[2]));
			}
			
		}
		if (upperCase($in.0.JOBLOCATION) == "ARLINGTON")
		{
			cityName = "ARLINGTON";
			stateAbbrev = "VA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "GREATER ATLANTA AREA")
		{
			cityName = "ATLANTA";
			stateAbbrev = "GA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "GREATER CHICAGO AREA")
		{
			cityName = "CHICAGO";
			stateAbbrev = "IL";
		}
		else if (upperCase($in.0.JOBLOCATION) == "GREATER DENVER AREA")
		{
			cityName = "DENVER";
			stateAbbrev = "CO";
		}
		else if (upperCase($in.0.JOBLOCATION) == "GREATER LOS ANGELES AREA")
		{
			cityName = "LOS ANGELES";
			stateAbbrev = "CA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "GREATER NEW YORK CITY AREA")
		{
			cityName = "NEW YORK";
			stateAbbrev = "NY";
		}
		else if (upperCase($in.0.JOBLOCATION) == "GREATER PHILADELPHIA AREA")
		{
			cityName = "PHILADELPHIA";
			stateAbbrev = "PA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "GREATER SAN DIEGO AREA")
		{
			cityName = "SAN DIEGO";
			stateAbbrev = "CA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "GREATER SEATTLE AREA")
		{
			cityName = "SAN DIEGO";
			stateAbbrev = "CA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "HUNT VALLEY")
		{
			cityName = "HUNT VALLEY";
			stateAbbrev = "MD";
		}
		else if (upperCase($in.0.JOBLOCATION) == "MIAMI/FORT LAUDERDALE AREA")
		{
			cityName = "MIAMI";
			stateAbbrev = "FL";
		}
		else if (upperCase($in.0.JOBLOCATION) == "MORRISVILLE")
		{
			cityName = "RALEIGH";
			stateAbbrev = "NC";
		}
		else if (upperCase($in.0.JOBLOCATION) == "NORFOLK")
		{
			cityName = "NORFOLK";
			stateAbbrev = "VA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "OAKLAND")
		{
			cityName = "OAKLAND";
			stateAbbrev = "CA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "PISCATAWAY")
		{
			cityName = "PISCATAWAY";
			stateAbbrev = "NJ";
		}
		else if (upperCase($in.0.JOBLOCATION) == "SAN FRANCISCO")
		{
			cityName = "SAN FRANCISCO";
			stateAbbrev = "CA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "SAN FRANCISCO BAY AREA")
		{
			cityName = "SAN FRANCISCO";
			stateAbbrev = "CA";
		}
		else if (upperCase($in.0.JOBLOCATION) == "WASHINGTON D.C. METRO AREA")
		{
			cityName = "WASHINGTON";
			stateAbbrev = "DC";
		}
		
		$out.0.JOBCITY = cityName;
		$out.0.JOBSTATEABBREV = stateAbbrev;
		$out.0.JOBSTATENAME = stateName;
	}
	$out.0.JOBPOSTINGDATE = str2date($in.0.JOBPOSTINGDATE,"MMM d, yyyy");
	if ($in.0.JOBTITLE != null)
	{	
		$out.0.JOBTITLE = replace($in.0.JOBTITLE,"[|]","");
	}
	$out.0.JOBURL = $in.0.JOBURL;

	return ALL;
}

