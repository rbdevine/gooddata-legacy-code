<Phase number="4">
<Node dbConnection="JDBC0" enabled="enabled" guiName="Populate Accumulation Table" guiX="249" guiY="401" id="POPULATE_INDUSTRY_QUEUE" type="DB_EXECUTE">
<attr name="sqlQuery"><![CDATA[insert into ${TMP_SUBSIDY_ACCUMULATED_COUNTS_TABLENAME} (
  ID,
  STATE_COUNTY_FIPS,
  TICKER,
  GOC,
  RATE_COLA,
  EMPLOYEE_COUNT
)
select 
  rank() over (order by ticker, state_county_fips, rate_cola, goc) id, 
  state_county_fips,
  ticker, 
  goc,
  rate_cola,
  employee_count
/*
from ${COUNTY_COUNT_TABLENAME} ec
join ${JOB_DISTRIBUTION_TABLENAME} d on 
  d.ticker = ec.ticker and (
    (ec.subsidiary is null and ec.ticker not in ('BBY','ICGH','KR','TGT','WMT'))
    or d.subsidiary = ec.subsidiary
  )
join lkp_just_co_secty_master m on m.ticker_trim = ec.ticker
join ${SUBSIDY_GOC_RATE_BY_COUNTY_TABLENAME} cjcd on 
  cjcd.state_county_fips = ec.state_county_fips and 
  cjcd.ticker = ec.ticker and 
  cjcd.goc = d.goc
*/
from ${SUBSIDY_GOC_RATE_BY_COUNTY_TABLENAME};]]></attr>
</Node>
<Node dbConnection="JDBC0" enabled="enabled" guiName="Populate Subsidy Counts Table" guiX="34" guiY="401" id="POPULATE_SUBSIDY_COUNTS_TABLE" type="DB_EXECUTE">
<attr name="sqlQuery"><![CDATA[insert into ${SUBSIDY_COUNTS_TABLENAME} (
  state_county_fips, 
  ticker,
  goc
)
select 
  gc.state_county_fips,
  gc.ticker,
  gc.goc
from ${SUBSIDY_GOC_RATE_BY_COUNTY_TABLENAME} gc
/*
from ${COUNTY_COUNT_TABLENAME} ec
join ${JOB_DISTRIBUTION_TABLENAME} d on 
  d.ticker = ec.ticker and (
    (ec.ticker in ('BBY','ICGH','KR','TGT','WMT') and 
      d.subsidiary = ec.subsidiary
    ) or 
    (ec.ticker not in ('BBY','ICGH','KR','TGT','WMT') and 
      ec.subsidiary is null
    )
  )
*/
join ${TMP_SUBSIDY_TICKER_QUEUE_TABLENAME} q on q.ticker = gc.ticker
group by 
  gc.state_county_fips, 
  gc.ticker, 
  gc.goc
order by 
  gc.state_county_fips, 
  gc.ticker, 
  gc.goc;]]></attr>
</Node>
</Phase>
<Phase number="5">
<Node dbConnection="JDBC0" enabled="enabled" guiName="Add EITC Counts" guiX="173" guiY="478" id="ADD_EITC_COUNTS" type="DB_EXECUTE">
<attr name="sqlQuery"><![CDATA[update ${SUBSIDY_COUNTS_TABLENAME} sc
set EMP_COUNT_EITC = gc.employee_count
from (
  select gc.state_county_fips, gc.ticker, gc.goc, gc.employee_count
/*
  select ec.state_county_fips, ec.ticker, ec.goc, ec.employee_count
  from (
    select state_county_fips, ticker, null subsidiary, goc, sum(job_employee_count) employee_count
    from (
      select state_county_fips, ec.ticker, ec.subsidiary, goc, d.distribution * ec.employee_count job_employee_count
      from ${COUNTY_COUNT_TABLENAME} ec
      join ${JOB_DISTRIBUTION_TABLENAME} d on d.ticker = ec.ticker and ((ec.ticker in ('BBY','ICGH','KR','TGT','WMT') and d.subsidiary = ec.subsidiary) or (ec.ticker not in ('BBY','ICGH','KR','TGT','WMT') and ec.subsidiary is null))
    ) t
    group by state_county_fips, ticker, goc
  ) ec
  join lkp_just_co_secty_master m on m.ticker_trim = ec.ticker
  join ${SUBSIDY_GOC_RATE_BY_COUNTY_TABLENAME} cjcd on 
    cjcd.state_county_fips = ec.state_county_fips and 
    cjcd.ticker = ec.ticker and 
    cjcd.goc = ec.goc
  left join ${SUBSIDY_EITC_TABLENAME} eitc on eitc.minimum_wage <= cjcd.rate_cola and cjcd.rate_cola <= eitc.maximum_wage
  where eitc.subsidy_annual is not null
) ec where sc.state_county_fips = ec.state_county_fips and sc.ticker = ec.ticker and sc.goc = ec.goc;
*/
  from ${SUBSIDY_GOC_RATE_BY_COUNTY_TABLENAME} gc
  left join ${SUBSIDY_EITC_TABLENAME} eitc on eitc.minimum_wage <= gc.rate_cola and gc.rate_cola <= eitc.maximum_wage
  where eitc.subsidy_annual is not null
) gc where sc.state_county_fips = gc.state_county_fips and sc.ticker = gc.ticker and sc.goc = gc.goc;]]></attr>
</Node>
</Phase>
<Phase number="6">
<Node dbConnection="JDBC0" enabled="disabled" guiName="Add SNAP Counts" guiX="171" guiY="551" id="ADD_SNAP_COUNTS" type="DB_EXECUTE">
<attr name="sqlQuery"><![CDATA[update ${SUBSIDY_COUNTS_TABLENAME} sc
set EMP_COUNT_SNAP = ec.employee_count
from (
  select ec.state_county_fips, ec.ticker, d.goc, ec.employee_count*d.distribution employee_count
  from ${COUNTY_COUNT_TABLENAME} ec
  join ${JOB_DISTRIBUTION_TABLENAME} d on 
    d.ticker = ec.ticker and (
      (ec.subsidiary is null and ec.ticker not in ('BBY','ICGH','KR','TGT','WMT'))
      or d.subsidiary = ec.subsidiary
    )
  join lkp_just_co_secty_master m on m.ticker_trim = ec.ticker
  join ${COMPANY_JOB_DATA_TABLENAME} cjd on cjd.just_co_id = m.just_co_id and cjd.job = d.goc
  join ${SUBSIDY_TICKER_CODE_TABLENAME} tc on tc.ticker = ec.ticker
  left join ${SUBSIDY_SNAP_TABLENAME} snap on snap.code = tc.code and snap.minimum_wage <= cjd.rate_raw and cjd.rate_raw <= snap.maximum_wage
  where snap.subsidy_annual is not null
) ec where sc.state_county_fips = ec.state_county_fips and sc.ticker = ec.ticker and sc.goc = ec.goc;]]></attr>
</Node>
