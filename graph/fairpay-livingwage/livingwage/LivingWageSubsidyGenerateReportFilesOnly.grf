<Node dbConnection="JDBC0" enabled="enabled" guiName="Select Details by County" guiX="27" guiY="312" id="SELECT_DETAILS_BY_COUNTY" type="DB_INPUT_TABLE">
<attr name="sqlQuery"><![CDATA[select
  $STATE_COUNTY_FIPS:=STATE_COUNTY_FIPS,
  $EMP_COUNT_TOTAL:=cast(round(sum(EMP_COUNT_TOTAL),3) as numeric(16,3)) EMP_COUNT_TOTAL,
  $EMP_COUNT_SUBSIDY_TOTAL:=cast(round(sum(EMP_COUNT_SUBSIDY_TOTAL),3) as numeric(16,3)) EMP_COUNT_SUBSIDY_TOTAL,
  $SUBSIDY_TOTAL:=concat('$',cast(round(sum(SUBSIDY_TOTAL),2) as numeric(16,2))) SUBSIDY_TOTAL,
  $SUBSIDY_BY_EMP_TOTAL:=concat('$',cast(round(case
    when sum(EMP_COUNT_SUBSIDY_TOTAL) > 0 then (sum(SUBSIDY_TOTAL)/sum(EMP_COUNT_SUBSIDY_TOTAL))/1500.0
    else 0.0
  end,2) as numeric(16,2))) SUBSIDY_BY_EMP_TOTAL,
  $EMP_COUNT_EITC:=cast(round(sum(EMP_COUNT_EITC),3) as numeric(16,3)) EMP_COUNT_EITC,
  $SUBSIDY_TOTAL_EITC:=concat('$',cast(round(sum(SUBSIDY_TOTAL_EITC),2) as numeric(16,2))) SUBSIDY_TOTAL_EITC,
  $SUBSIDY_BY_EMP_EITC:=concat('$',cast(round(case
    when sum(EMP_COUNT_EITC) > 0 then (sum(SUBSIDY_TOTAL_EITC)/sum(EMP_COUNT_EITC))/1500.0
    else 0.0
  end,2) as numeric(16,2))) SUBSIDY_BY_EMP_EITC,
  $EMP_COUNT_SNAP:=cast(round(sum(EMP_COUNT_SNAP),3) as numeric(16,3)) EMP_COUNT_SNAP,
  $SUBSIDY_TOTAL_SNAP:=concat('$',cast(round(sum(SUBSIDY_TOTAL_SNAP),2) as numeric(16,2))) SUBSIDY_TOTAL_SNAP,
  $SUBSIDY_BY_EMP_SNAP:=concat('$',cast(round(case
    when sum(EMP_COUNT_SNAP) > 0 then (sum(SUBSIDY_TOTAL_SNAP)/sum(EMP_COUNT_SNAP))/1500.0
    else 0.0
  end,2) as numeric(16,2))) SUBSIDY_BY_EMP_SNAP,
  $EMP_COUNT_ADULT_MEDICAID:=cast(round(sum(EMP_COUNT_ADULT_MEDICAID),3) as numeric(16,3)) EMP_COUNT_ADULT_MEDICAID,
  $SUBSIDY_TOTAL_ADULT_MEDICAID:=concat('$',cast(round(sum(SUBSIDY_TOTAL_ADULT_MEDICAID),2) as numeric(16,2))) SUBSIDY_TOTAL_ADULT_MEDICAID,
  $SUBSIDY_BY_EMP_ADULT_MEDICAID:=concat('$',cast(round(case
    when sum(EMP_COUNT_ADULT_MEDICAID) > 0 then (sum(SUBSIDY_TOTAL_ADULT_MEDICAID)/sum(EMP_COUNT_ADULT_MEDICAID))/1500.0
    else 0.0
  end,2) as numeric(16,2))) SUBSIDY_BY_EMP_ADULT_MEDICAID,
  $EMP_COUNT_CHILD_MEDICAID:=cast(round(sum(EMP_COUNT_CHILD_MEDICAID),3) as numeric(16,3)) EMP_COUNT_CHILD_MEDICAID,
  $SUBSIDY_TOTAL_CHILD_MEDICAID:=concat('$',cast(round(sum(SUBSIDY_TOTAL_CHILD_MEDICAID),2) as numeric(16,2))) SUBSIDY_TOTAL_CHILD_MEDICAID,
  $SUBSIDY_BY_EMP_CHILD_MEDICAID:=concat('$',cast(round(case
    when sum(EMP_COUNT_CHILD_MEDICAID) > 0 then (sum(SUBSIDY_TOTAL_CHILD_MEDICAID)/sum(EMP_COUNT_CHILD_MEDICAID))/1500.0
    else 0.0
  end,2) as numeric(16,2))) SUBSIDY_BY_EMP_CHILD_MEDICAID,
  $EMP_COUNT_ACA:=cast(round(sum(EMP_COUNT_ACA),3) as numeric(16,3)) EMP_COUNT_ACA,
  $SUBSIDY_TOTAL_ACA:=concat('$',cast(round(sum(SUBSIDY_TOTAL_ACA),2) as numeric(16,2))) SUBSIDY_TOTAL_ACA,
  $SUBSIDY_BY_EMP_ACA:=concat('$',cast(round(case
    when sum(EMP_COUNT_ACA) > 0 then (sum(SUBSIDY_TOTAL_ACA)/sum(EMP_COUNT_ACA))/1500.0
    else 0.0
  end,2) as numeric(16,2))) SUBSIDY_BY_EMP_ACA
from ${SUBSIDY_DETAILS_TABLENAME}
group by state_county_fips 
order by state_county_fips;]]></attr>
