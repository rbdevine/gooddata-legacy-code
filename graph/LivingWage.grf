<attr name="transform"><![CDATA[//#CTL2

// Transforms input record into output record.
function integer transform() {
	$out.0.state = $in.0.state;
	$out.0.county_metro = $in.0.county_metro;
	$out.0._1a = $in.0._1a;
	$out.0._1a1c = $in.0._1a1c;
	$out.0._1a2c = $in.0._1a2c;
	$out.0._1a3c = $in.0._1a3c;
	$out.0._2a1w = $in.0._2a1w;
	$out.0._2a1w1c = $in.0._2a1w1c;
	$out.0._2a1w2c = $in.0._2a1w2c;
	$out.0._2a1w3c = $in.0._2a1w3c;
	$out.0._2a2w = $in.0._2a2w;
	$out.0._2a2w1c = $in.0._2a2w1c;
	$out.0._2a2w2c = $in.0._2a2w2c;
	$out.0._2a2w3c = $in.0._2a2w3c;
	$out.0.url = $in.0.url;
	$out.0.juststatename = upperCase($in.0.state);
	string county = upperCase($in.0.county_metro);
	// if we have city state in the county field
	if (indexOf(county,",") > 1)
	{
		$out.0.justcountyname = left(county,indexOf(county,",")) + " COUNTY";
	}
	else
	{
		$out.0.justcountyname = county;
	}
	return ALL;
}

